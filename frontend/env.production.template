# Production Environment Variables Template
# Copy this file to .env.production and fill in your actual values

# API Configuration
VITE_API_BASE_URL=https://your-backend-url.herokuapp.com/api

# Socket Configuration
VITE_SOCKET_URL=https://your-backend-url.herokuapp.com

# Google OAuth Configuration (Add your actual Google Client ID)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Facebook OAuth Configuration (Add your actual Facebook App ID)
VITE_FACEBOOK_APP_ID=your_facebook_app_id_here 